{
  "name": "vg-lan-smash",
  "version": "0.0.0",
  "description": "Bundle for Visiongaming LAN Smash",
  "homepage": "https://visiongaming.ch",
  "author": {
    "name": "jschuler",
    "email": "jeremy.schuler@vjss.ch",
    "url": "https://vjss.ch"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "vg",
    "lan",
    "smash",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "dashboardPanels": [
      {
        "name": "smash",
        "title": "Smash",
        "width": 8,
        "file": "smash.html",
        "workspace": "Smash",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "1v1.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      },
      {
        "file": "1v1_nocam.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      },
      {
        "file": "big_cams.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ],
    "mount": [
      {
        "directory": "assets",
        "endpoint": "assets"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT"
}
